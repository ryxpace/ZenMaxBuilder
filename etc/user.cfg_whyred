EVA_ARM64_BRANCH="ZenMaxBuilder"
EVA_ARM64_VERSION="eva-arm64/lib/gcc/aarch64-elf"
EVA_ARM64_CHECK="gcc-arm64/bin/aarch64-elf-gcc"

