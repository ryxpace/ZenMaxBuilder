EVA_ARM_BRANCH="ZenMaxBuilder"
EVA_ARM_VERSION="eva-arm/lib/gcc/arm-eabi"
EVA_ARM_CHECK="gcc-arm/bin/arm-eabi-gcc"

# LOS GCC-ARM64 (fork of LineageOS updated every 15min)
